<!DOCTYPE html>
<head>
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8">
  {{> head}}
  <!-- my js & css -->
	<link href='https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,700' rel='stylesheet' type='text/css'>

    <script src="js/multiline5.js"></script>
	<link href='css/multiline.css' rel='stylesheet' type='text/css'>

</head>
<body>
  {{> header}}

  {{> start}}
  <!-- my charts -->
	<div class="ui segment">

		<div class="ui active inverted dimmer" id="wait">
			<div class="ui large text loader">Loading</div>
		</div>

		<div class="ui grid">
			<div class="two height row">
			</div>
			<div class="twelve height row">
				<div class="multilineChart twelve wide column"></div>
				<div class="multilineChartSelectorTop four wide column">

					<div class="row">
						<div class="ui header" id="vgName">Selection</div>
					</div>

					<div class="row">
						<div class="multilineChartSelector"></div>
					</div>

					<div class="typhoonbutton">
						<div class="row">
							<button class="ui button typhoonbutton">OFF</button>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>

  {{> end}}
    <!-- my js & css -->
    {{> footer}}

    <script>

    	$().ready(function (){
            drawMultilineChart(d3.select(".multilineChart"),
				d3.select(".multilineChartSelector"),
				$(".multilineChart").width(),
				$(".multilineChart").height());
    	});

    </script>


</body>
